<package format="2">
  <name>tile_map</name>
  <version>0.0.1</version>
  <description>

     Tile map provides a slippy map style interface for visualizing 
     OpenStreetMap and GooleMap tiles.  A mapviz visualization plug-in is also
     implemented

  </description>
  <author>Marc Alban</author>
  <maintainer email="malban@swri.org">Marc Alban</maintainer>
  <license>BSD</license>
  <url>https://github.com/swri-robotics/mapviz</url>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>roscpp</depend>
  <depend>tf</depend>
  <depend>pluginlib</depend>
  <depend>mapviz</depend>
  <depend>math_util</depend>
  <depend>transform_util</depend>
  <depend>build_tools</depend>
  <depend>libqt4-dev</depend>
  <depend>qt4-qmake</depend>

  <export>
    <mapviz plugin="${prefix}/mapviz_plugins.xml" />
  </export>

</package>


